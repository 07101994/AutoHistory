{
  "version": "1.0.0-*",
  "dependencies": {
    "dotnet-test-xunit": "2.2.0-*",
    "xunit": "2.2.0-*",
    "Microsoft.EntityFrameworkCore.SqlServer": "1.1.0-*",
    "Microsoft.EntityFrameworkCore.AutoHistory": {
      "target": "project"
    }
  },

  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.*"
        }
      }
    }
  },

  "buildOptions": {
    "copyToOutput": {
      "include": [ "xunit.runner.json", "appsetting.json" ]
    },
    "xmlDoc": true,
    "nowarn": [ "CS1591" ]
  },

  "testRunner": "xunit"
}
